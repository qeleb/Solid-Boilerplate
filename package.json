{
  "name": "solid-boilerplate",
  "version": "0.1.0",
  "description": "Solid Boilerplate",
  "author": "qeleb",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "browserslist": {
    "production": [
      "firefox >=103",
      "chrome >=88",
      "safari >=14.1",
      "ios_saf >=14.5"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}": "eslint --fix",
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "prettier --write --list-different"
    ]
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "build:analyze": "ANALYZE=true && vite build",
    "test": "vitest run --coverage",
    "test:ui": "vitest --coverage --ui",
    "lint": "eslint src && stylelint src/**/*.scss",
    "lint:fix": "eslint --fix src && stylelint --fix src/**/*.scss",
    "preinstall": "npx only-allow yarn",
    "postinstall": "husky"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.7",
    "@testing-library/jest-dom": "^6.4.2",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitest/coverage-v8": "^1.5.0",
    "@vitest/ui": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-regexp": "^2.5.0",
    "eslint-plugin-solid": "^0.13.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-vitest": "^0.5.3",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lightningcss": "^1.24.1",
    "lint-staged": "^15.2.2",
    "miragejs": "^0.1.48",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.75.0",
    "stylelint": "^16.3.1",
    "stylelint-config-hudochenkov": "^10.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "stylelint-order": "^6.0.4",
    "terser": "^5.30.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-optimize-css-modules": "^1.0.6",
    "vite-plugin-sass-dts": "^1.3.17",
    "vite-plugin-solid": "2.8.2",
    "vite-plugin-svgo": "^1.4.0",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@solidjs/router": "^0.13.2",
    "solid-js": "^1.8.16",
    "solidjs-use": "^2.3.0"
  },
  "resolutions": {
    "svgo": "^3.2.0"
  },
  "packageManager": "yarn@3.7.0"
}
