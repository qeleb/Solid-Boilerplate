{
  "name": "solid-boilerplate",
  "version": "0.1.0",
  "description": "Solid Boilerplate",
  "author": "qeleb",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "browserslist": {
    "production": [
      "firefox >=103",
      "chrome >=88",
      "safari >=14.1",
      "ios_saf >=14.5"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{html,json,yaml}": "prettier --write --list-different",
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}": "eslint --fix",
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "prettier --write --list-different"
    ]
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "build:analyze": "ANALYZE=true && vite build",
    "test": "vitest run --coverage",
    "test:ui": "vitest --coverage --ui",
    "lint": "eslint src && stylelint src/**/*.scss",
    "lint:fix": "eslint --fix src && stylelint --fix src/**/*.scss",
    "preinstall": "npx only-allow yarn",
    "postinstall": "husky"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.25.2",
    "@solidjs/testing-library": "^0.8.9",
    "@testing-library/jest-dom": "^6.5.0",
    "@types/node": "^20.16.5",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-regexp": "^2.6.0",
    "eslint-plugin-solid": "^0.14.3",
    "eslint-plugin-testing-library": "^6.3.0",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.1.5",
    "jsdom": "^25.0.0",
    "lightningcss": "^1.26.0",
    "lint-staged": "^15.2.10",
    "miragejs": "^0.1.48",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "npm:sass-embedded@^1.78.0",
    "stylelint": "^16.9.0",
    "stylelint-config-hudochenkov": "^10.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "stylelint-order": "^6.0.4",
    "terser": "^5.31.6",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-optimize-css-modules": "^1.1.0",
    "vite-plugin-sass-dts": "^1.3.25",
    "vite-plugin-solid": "^2.10.2",
    "vite-plugin-svgo": "1.4.0",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@solidjs/router": "0.10.1",
    "solid-js": "1.8.5",
    "solidjs-use": "^2.3.0"
  },
  "resolutions": {
    "caniuse-lite": "1.0.30001654",
    "svgo": "^3.3.2"
  },
  "packageManager": "yarn@3.8.3"
}
