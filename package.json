{
  "name": "solid-boilerplate",
  "version": "0.1.0",
  "description": "Solid Boilerplate",
  "author": "qeleb",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "browserslist": {
    "production": [
      "chrome >=108",
      "firefox >=121",
      "safari >=16.4",
      "ios_saf >=16.4"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{html,json,yaml}": "prettier --write --list-different",
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}": "eslint --fix",
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "prettier --write --list-different"
    ]
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "build:analyze": "ANALYZE=true && vite build",
    "test": "vitest run --coverage",
    "lint": "eslint src && stylelint src/**/*.scss",
    "lint:fix": "eslint --fix src && stylelint --fix src/**/*.scss",
    "preinstall": "npx only-allow yarn",
    "postinstall": "husky"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.27.1",
    "@rollup/plugin-swc": "^0.4.0",
    "@solidjs/testing-library": "^0.8.10",
    "@swc/core": "^1.11.24",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^22.15.21",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/eslint-plugin": "1.2.0",
    "@vitest/ui": "^3.1.4",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-testing-library": "^7.2.1",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lightningcss": "^1.30.1",
    "lint-staged": "^16.0.0",
    "miragejs": "^0.1.48",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass-embedded": "^1.89.0",
    "stylelint": "^16.19.1",
    "stylelint-config-hudochenkov": "^12.0.0",
    "stylelint-config-recommended-scss": "^15.0.1",
    "stylelint-no-unsupported-browser-features": "^8.0.4",
    "stylelint-order": "^7.0.0",
    "terser": "^5.39.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-optimize-css-modules": "^1.2.0",
    "vite-plugin-sass-dts": "^1.3.31",
    "vite-plugin-solid": "^2.11.6",
    "vite-plugin-svgo": "^2.0.0",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@solidjs/router": "0.10.1",
    "solid-js": "1.8.5",
    "solidjs-use": "^2.3.0"
  },
  "resolutions": {
    "@babel/helper-module-imports": "^7.27.1",
    "aria-query": "^5.3.2",
    "emoji-regex": "latest",
    "known-css-properties": "^0.36.0",
    "mdn-data": "latest"
  },
  "packageManager": "yarn@4.9.1"
}
