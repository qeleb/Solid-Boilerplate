{
  "name": "solid-boilerplate",
  "version": "0.1.0",
  "description": "Solid Boilerplate",
  "author": "qeleb",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">=18.x"
  },
  "browserslist": {
    "production": [
      "firefox >=103",
      "chrome >=88",
      "safari >=14.1",
      "ios_saf >=14.5"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{html,json,yaml}": "prettier --write --list-different",
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}": "eslint --fix",
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "prettier --write --list-different"
    ]
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "build:analyze": "ANALYZE=true && vite build",
    "test": "vitest run --coverage",
    "lint": "eslint src && stylelint src/**/*.scss",
    "lint:fix": "eslint --fix src && stylelint --fix src/**/*.scss",
    "preinstall": "npx only-allow yarn",
    "postinstall": "husky"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.26.8",
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^22.13.5",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/eslint-plugin": "1.1.35",
    "@vitest/ui": "^3.0.7",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "browserslist-to-esbuild": "^2.1.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lightningcss": "^1.29.1",
    "lint-staged": "^15.4.3",
    "miragejs": "^0.1.48",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass-embedded": "^1.85.1",
    "stylelint": "^16.14.1",
    "stylelint-config-hudochenkov": "^11.0.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-no-unsupported-browser-features": "^8.0.4",
    "stylelint-order": "^6.0.4",
    "terser": "^5.39.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-optimize-css-modules": "^1.2.0",
    "vite-plugin-sass-dts": "^1.3.30",
    "vite-plugin-solid": "^2.11.4",
    "vite-plugin-svgo": "^1.6.2",
    "vitest": "^3.0.7"
  },
  "dependencies": {
    "@solidjs/router": "0.10.1",
    "solid-js": "1.8.5",
    "solidjs-use": "^2.3.0"
  },
  "resolutions": {
    "@babel/helper-module-imports": "^7.25.9",
    "aria-query": "^5.3.2",
    "emoji-regex": "latest",
    "esbuild": "^0.24.2",
    "known-css-properties": "^0.35.0",
    "mdn-data": "latest",
    "svgo": "^3.3.2"
  },
  "packageManager": "yarn@3.8.7"
}
