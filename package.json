{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "React Boilerplate",
  "author": "qeleb",
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "node": ">=16.x"
  },
  "browserslist": {
    "production": [
      "firefox >=103",
      "chrome >=88",
      "safari >=14.1",
      "ios_saf >=14.5"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,mjs,cts,mts}": "eslint --fix",
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "prettier --write --list-different"
    ]
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "vite build",
    "build:analyze": "ANALYZE=true && vite build",
    "test": "vitest run --coverage",
    "test:ui": "vitest --coverage --ui",
    "lint": "eslint src && stylelint src/**/*.scss",
    "lint:fix": "eslint --fix src && stylelint --fix src/**/*.scss",
    "preinstall": "npx only-allow yarn",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-regexp": "^2.1.1",
    "eslint-plugin-testing-library": "^6.1.2",
    "eslint-plugin-vitest": "^0.3.9",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.1.0",
    "miragejs": "^0.1.48",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-hudochenkov": "^9.0.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-no-unsupported-browser-features": "^7.0.0",
    "stylelint-order": "^6.0.3",
    "terser": "^5.24.0",
    "typescript": "5.1.6",
    "vite": "^4.5.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-optimize-css-modules": "^1.0.4",
    "vite-plugin-sass-dts": "^1.3.11",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "preact": "10.11.3",
    "react-router-dom": "^6.18.0"
  },
  "packageManager": "yarn@3.6.4"
}
